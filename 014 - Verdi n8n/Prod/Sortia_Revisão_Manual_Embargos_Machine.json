        "sqlQuery": "WITH\nquery_1 AS (\n  SELECT COUNT(*) AS qtd\n  FROM `pdme000426-c1s7scatwm0-furyid.STG.SALESFORCE_EMBARGOS_REVISAO_MANUAL` sf\n  LEFT JOIN `meli-bi-data.WHOWNER.BT_SORTIA_EMBARGOS` st\n    ON CAST(sf.issue_number AS STRING) = CAST(st.issue AS STRING)\n  WHERE sf.legales_approval_status <> 'Approved'\n    AND st.issue IS NULL\n),\n\nquery_2 AS (\n  SELECT COUNT(*) AS qtd\n  FROM `pdme000426-c1s7scatwm0-furyid.STG.SALESFORCE_EMBARGOS_REVISAO_MANUAL` sf\n  JOIN `meli-bi-data.WHOWNER.BT_SORTIA_EMBARGOS` st\n    ON CAST(sf.issue_number AS STRING) = CAST(st.issue AS STRING)\n  WHERE sf.legales_approval_status <> 'Approved'\n    AND st.ANALISIS_RESULT = 'REVISION_MANUAL'\n),\n\nquery_3 AS (\n  SELECT COUNT(*) AS qtd\n  FROM (\n    SELECT issue_number AS issue\n    FROM `pdme000426-c1s7scatwm0-furyid.STG.SALESFORCE_EMBARGOS_REVISAO_MANUAL`\n    WHERE legales_approval_status = 'Approved'\n  ) sf\n  JOIN (\n    SELECT ISSUE\n    FROM `meli-bi-data.WHOWNER.BT_SORTIA_EMBARGOS`\n    WHERE ANALISIS_RESULT <> 'REVISION_MANUAL'\n  ) sortia\n    ON sortia.ISSUE = sf.issue\n),\n\nquery_4 AS (\n  SELECT COUNT(*) AS qtd\n  FROM (\n    SELECT issue_number AS issue\n    FROM `pdme000426-c1s7scatwm0-furyid.STG.SALESFORCE_EMBARGOS_REVISAO_MANUAL`\n    WHERE legales_approval_status <> 'Approved'\n  ) sf\n  JOIN (\n    SELECT ISSUE\n    FROM `meli-bi-data.WHOWNER.BT_SORTIA_EMBARGOS`\n    WHERE ANALISIS_RESULT <> 'REVISION_MANUAL'\n  ) sortia\n    ON sortia.ISSUE = sf.issue\n)\n\nSELECT\n  'Query 1' AS query,\n  qtd AS quantidade_linhas,\n  CURRENT_DATETIME() AS data_update\nFROM query_1\n\nUNION ALL\n\nSELECT\n  'Query 2',\n  qtd,\n  CURRENT_DATETIME()\nFROM query_2\n\nUNION ALL\n\nSELECT\n  'Query 3',\n  qtd,\n  CURRENT_DATETIME()\nFROM query_3\n\nUNION ALL\n\nSELECT\n  'Query 4',\n  qtd,\n  CURRENT_DATETIME()\nFROM query_4\n",
        "options": {}
      },
